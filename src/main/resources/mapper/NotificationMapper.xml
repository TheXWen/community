  <select id="selectInFoByQuestionId" parameterType="java.lang.Integer" resultType="com.xw.community.dto.NotificationDTO">
    SELECT n.id,n.type,n.gmt_create,u.`name` AS senderName,n.`status`,q.title AS outerTitle,q.id AS questionId
    FROM notification AS n
    INNER JOIN question AS q ON n.outerId = q.id
    INNER JOIN `user` AS u ON n.sender = u.id
    WHERE n.id = #{id}
  </select>

  <select id="selectInFoByCommentId" parameterType="java.lang.Integer" resultType="com.xw.community.dto.NotificationDTO">
    SELECT n.id,n.type,n.gmt_create,u.`name` AS senderName,n.`status`,c.content AS outerTitle,q.id AS questionId
    FROM notification AS n
    INNER JOIN `comment` AS c ON n.outerId = c.id
    INNER JOIN `user` AS u ON n.sender = u.id
    INNER JOIN question AS q ON c.parent_id = q.id
    WHERE n.id = #{id}
  </select>
